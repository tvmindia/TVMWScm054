
@{
    ViewBag.Title = "Monthly Income And Expenditure Report";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@using SCManager.UserInterface.App_Start

@section styles {
    @Styles.RenderFormat(SiteKeys.StyleVersion,"~/Content/DataTables/css/datatable")
    @Styles.RenderFormat(SiteKeys.StyleVersion,"~/Content/DataTables/css/datatableButtons")
    @Styles.RenderFormat(SiteKeys.StyleVersion,"~/Content/bootstrapdatepicker")
}


<div class="col-md-12">
    <div class="SCHeader col-md-6">

    </div>
    <div class="col-md-6">

    </div>


</div>
<div id="exTab3" class="container SCtabContainer">
    <ul class="nav nav-pills"></ul>
    <div class="tab-content clearfix">
        <div class="tab-pane fade in active" id="IncomExpenseList">
            <div class="col-md-12">&nbsp;</div>
            <div class="col-md-8 ">
                <div class="col-md-12 filterHead ">Filter</div>
                <div class="col-md-12 ">
                    <div id="external_filter" class="form-inline">
                        <label><b>From Date</b></label>
                        <input class="form-control datepicker" onchange="RefreshIncomeExpenseSummaryTable()" style="width:190px" name="fromdate" id="fromdate" value=@ViewBag.fromdate type="Date">
                        <label><b>To Date</b> </label>
                        <input class="form-control datepicker" onchange="RefreshIncomeExpenseSummaryTable()" style="width:190px" name="todate" id="todate" value=@ViewBag.todate type="Date">

                    </div>
                </div>
            </div>

            <div class="col-md-4 ">
                <div class="" id="btnPatchReport" style="min-height:25px">
                    @{
                        var data = new { ActionType = "List" };
                        @Html.Action("ChangeButtonStyle", "Report", data);
                    }
                </div>
            </div>

            <div class="col-md-12">&nbsp;</div>

            <div class="col-md-12">
                <div class="col-md-12 ReportHead">Monthly Income&Expenditure Report</div>
                <div class="container-fluid">
                    <div class="row">

                        <table id="tblIncomeExpense" class="table compact" cellspacing="0" width="100%">
                            <thead>
                                <tr class="text-center">
                                    <th>Reference No</th>
                                    <th>Description</th>
                                    <th>Account Head</th>
                                    <th>Income(₹)</th>
                                    <th>Expense(₹)</th>
                                    <th>Balance(₹)</th>
                                    <th>Date</th>
                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>


            </div>


        </div>

    </div>
</div>

@section Scripts {
 @Scripts.RenderFormat(SiteKeys.ScriptVersion,"~/bundles/datatable")
 @Scripts.RenderFormat(SiteKeys.ScriptVersion,"~/bundles/jsZip")
 @Scripts.RenderFormat(SiteKeys.ScriptVersion,"~/bundles/datatableButtons")
 @Scripts.RenderFormat(SiteKeys.ScriptVersion,"~/bundles/bootstrapdatepicker")
 @Scripts.RenderFormat(SiteKeys.ScriptVersion,"~/bundles/IncomeExpense")
}






