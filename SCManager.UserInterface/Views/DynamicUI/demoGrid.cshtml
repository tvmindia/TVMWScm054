
@{
    ViewBag.Title = "demoGrid";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

@section styles {
    @Styles.Render("~/Content/DataTables/css/datatable")
    @Styles.Render("~/Content/bootstrapdatepicker")

}
<div class="col-md-12"> <div class="col-md-2">Last change</div><div class="col-md-2"><div id="last"></div></div>  </div>
